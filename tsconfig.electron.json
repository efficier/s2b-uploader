{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/electron",
    "rootDir": "./src/electron",
    "sourceMap": false,
    "noImplicitAny": false,
    "strict": false
  },
  "include": [
    "src/electron/**/*"
  ]
}
